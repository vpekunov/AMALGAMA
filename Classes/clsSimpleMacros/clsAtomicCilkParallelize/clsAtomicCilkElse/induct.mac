@fast(db_gid,"db_gid.csv").
@fast(db_atomic_prog,"db_atomic_prog.csv").
@fast(db_atomic_arg,"db_atomic_arg.csv").
@context(PREV,infinity):-
  (((\;)+|\}|\\n)((\s|\\t)*\\n)*)(\s|\\t)*.
@glue:-.
@global_unique(ELSE,1):-
  (else).
@glue:-.
@context(NEXT,1):-
  ()->{GID}
  (\s|\\t)*
  ((\s|\{|\\n|\\t)
     ()?=>{db_gid($GID)}
     ()-=>{db_atomic_prog(GID,_)}
  )+=>{db_atomic_prog(GID,"else")}
  (\s|\\t)*.