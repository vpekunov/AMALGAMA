@fast(db_gid,"db_gid.csv").
@fast(db_cilk_prog,"db_cilk_prog.csv").
@fast(db_cilk_arg,"db_cilk_arg.csv").
@context(CONTEXT,infinity):-
  ((^)|\\n)(\s|\\t)*.
@glue:-.
@global_unique(PREPROC,1):-
  ()->{GID}
  (\s|\\t)*
  (\#((include\s*\<[^\>]+\>\s*)|([^\\]*\\\\\\n)*[^\\]*)(\\n|($))
     ()?=>{db_gid($GID)}
     ()-=>{db_cilk_arg(GID,_,_),db_cilk_prog(GID,_)}
  )+=>{db_cilk_arg(GID,"body",$),db_cilk_prog(GID,"preproc")}
  (\s|\\t)*.
