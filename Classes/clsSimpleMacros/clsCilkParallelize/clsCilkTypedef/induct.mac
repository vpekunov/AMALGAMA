@fast(db_gid,"db_gid.csv").
@fast(db_cilk_prog,"db_cilk_prog.csv").
@fast(db_cilk_arg,"db_cilk_arg.csv").
@context(PREV,infinity):-
  (((^)|(\;)+|\}|\)|\{|\\n)((\s|\\t)*\\n)*)(\s|\\t)*.
@glue:-.
@global_unique(TYPE,1):-
  ()->{GID}
  (\s|\\t)*
  typedef((\\n|\s|\\t)+)
  (((.{1,2048}?)->{EXPR}\;)?=>{Predicates.BAL($,';')}
       ()?=>{db_gid($GID)}
       ()-=>{db_cilk_prog(GID,_),db_cilk_arg(GID,_,_)}
       )+=>{db_cilk_prog(GID,"type"),db_cilk_arg(GID,"def",EXPR)}
  (\s|\\t)*.
