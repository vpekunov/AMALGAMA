@fast(db_gid,"db_gid.csv").
@fast(db_gpu_prog,"db_gpu_prog.csv").
@fast(db_gpu_arg,"db_gpu_arg.csv").
@fast(db_gpu_incl,"db_gpu_incl.csv").
@context(CONTEXT,infinity):-
  ((^)|\\n)(\s|\\t)*.
@glue:-.
@unique(INCL,1):-
  (\s|\\t)*
  (\#include(\s|\\t)*\x22([^\x22]+)->{FILE}\x22(\\n|($))
     ()?=>{!db_gpu_incl(FILE)}
  )+=>{db_gpu_incl(FILE)}
  (\s|\\t)*.
@replace(INCL):-$(${FILE}).
