@fast(db_gid,"db_gid.csv").
@fast(db_atomic_prog,"db_atomic_prog.csv").
@fast(db_atomic_arg,"db_atomic_arg.csv").
@fast(db_atomic_incl,"db_atomic_incl.csv").
@context(CONTEXT,infinity):-
  ((^)|\\n)(\s|\\t)*.
@glue:-.
@unique(INCL,1):-
  (\s|\\t)*
  (\#include(\s|\\t)*\x22([^\x22]+)->{FILE}\x22(\\n|($))
     ()?=>{!db_atomic_incl(FILE)}
  )+=>{db_atomic_incl(FILE)}
  (\s|\\t)*.
@replace(INCL):-$(${FILE}).
